{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
    "Default": "Information",
    "Override": {
      "Microsoft":"Information",
      "System": "Information"
    }
  },
  "WriteTo": [
    { "Name": "Console" },
    { "Name": "File", "Args": { "path": "Logs/log.txt", "rollingInterval": "Day"} }
  ]
},
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "http://localhost:5261",
    "Audience": "http://localhost:5261",
    "Key": "key"
  }
}
